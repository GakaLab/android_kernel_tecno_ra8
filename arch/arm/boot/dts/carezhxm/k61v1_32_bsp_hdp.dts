&charger {
	algorithm_name = "LinearCharging";
	enable_sw_jeita;
	battery_cv = <4350000>;
	max_charger_voltage = <6000000>;
	ac_charger_input_current = <1200000>;
	ac_charger_current = <1200000>;
	charging_host_charger_current = <1200000>;
	ta_ac_charger_current = <1200000>;

	/* sw jeita */
	jeita_temp_above_t4_cv = <4100000>;
	jeita_temp_t3_to_t4_cv = <4100000>;
	jeita_temp_t2_to_t3_cv = <4350000>;
	jeita_temp_t1_to_t2_cv = <4350000>;
	jeita_temp_t0_to_t1_cv = <4350000>;
	jeita_temp_below_t0_cv = <4100000>;

	jeita_temp_above_t4_chg_current = <0>;
	jeita_temp_t3_to_t4_chg_current = <600000>;
	jeita_temp_t2_to_t3_chg_current = <1200000>;
	jeita_temp_t1_to_t2_chg_current = <600000>;
	jeita_temp_t0_to_t1_chg_current = <600000>;
	jeita_temp_below_t0_chg_current = <0>;

	temp_t4_thres = <55>;
	temp_t4_thres_minus_x_degree = <52>;
	temp_t3_thres = <45>;
	temp_t3_thres_minus_x_degree = <42>;
	temp_t2_thres = <15>;
	temp_t2_thres_plus_x_degree = <18>;
	temp_t1_thres = <5>;
	temp_t1_thres_plus_x_degree = <8>;
	temp_t0_thres = <0>;
	temp_t0_thres_plus_x_degree = <3>;
	temp_neg_10_thres = <0>;

	/* battery temperature protection */
	enable_min_charge_temp;
	min_charge_temp = <0>;
	min_charge_temp_plus_x_degree = <3>;
	max_charge_temp = <55>;
	max_charge_temp_minus_x_degree = <52>;

	/* linear charger */
	recharge_offset = <100000>;
	topoff_voltage = <4200000>;
	chg_full_current = <150000>;
};

